{
  "redirects": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "swap.dev.br" }],
      "destination": "https://app.swap.dev.br",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.swap.dev.br" }],
      "destination": "https://swap.dev.br/$1",
      "permanent": true
    }
  ]
}
